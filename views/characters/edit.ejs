<h2 class="page-header">Edit Character</h2>
<form action="/characters/<%= character.id %>?_method=PUT" method="post">
    <%- include('_form_fields') %>
    <div class="form-row form-row-end btn-row">
        <a class="btn btn-danger" href="/characters/<%= character.id %>">Cancel</a>
        <button class="btn btn-primary" type="submit">Update</button>
    </div>
</form>
