<%- include('index') %>
<h2 class="page-header">Abandoned <%= animes.length %></h2>
<div class="content-table">
    <table>
        <thead>
            <%- include('_anime_table_thead') %>
        </thead>
        <tbody>
            <% animes.forEach(anime => { %>
                <%- include('_anime_table_tbody', { anime: anime }) %>
            <% }) %>
        </tbody>
    </table>
</div>
